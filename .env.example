# Port configuration (optional, defaults to 3000)
PORT=3003

# Supabase Configuration
# Get these from: https://app.supabase.com/project/_/settings/api
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Plaid Configuration
# Get these from: https://dashboard.plaid.com/account/keys
REACT_APP_PLAID_CLIENT_ID=your_plaid_client_id_here
REACT_APP_PLAID_SECRET=your_plaid_secret_here
REACT_APP_PLAID_ENVIRONMENT=sandbox  # Options: sandbox, development, production

# Optional: Anthropic API for AI features
# Get from: https://console.anthropic.com/account/keys
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here